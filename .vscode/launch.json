{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: pastebin-bgtasks (app.py)",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/pastebin-bgtasks/app.py",
      "console": "integratedTerminal"
    },
    {
      "name": "Python: pastebin-app (pastebin-app.py)",
      "type": "python",
      "request": "launch",
      "module": "flask",
      "env": {
        "FLASK_APP": "pastebin-app.py",
        "FLASK_ENV": "development",
        "FLASK_DEBUG": "1"
      },
      "args": [
        "run",
        "--port=5001"
      ],
      "jinja": true,
      "cwd": "${workspaceFolder}/pastebin-app"
    }
  ]
}